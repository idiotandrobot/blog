---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
    {% for post in site.posts limit:10 %}
      <item>
        <title>{% if post.link %}Link: {% elsif post.project %}GitHub: {% endif %}{{ post.title | xml_escape }}</title>
        <description>{{ post.content | xml_escape }}{% include post/links.html links = post.links -%}</description>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>{% if post.link %}{{ post.link }}{% elsif post.project %}https://github.com/{{ site.github_user }}/{{ post.project }}{% else %}{{ site.url }}{{ post.url }}{% endif %}</link>
        <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
